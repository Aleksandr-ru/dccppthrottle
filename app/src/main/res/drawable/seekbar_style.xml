<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background"
        android:top="30dp"
        android:bottom="30dp">
        <shape android:shape="rectangle">
            <corners android:radius="5dp" />
            <gradient
                android:angle="270"
                android:startColor="#33000000"
                android:centerColor="#11000000"
                android:endColor="#11000000"
                android:centerY="0.2"
                android:type="linear"
                />
        </shape>
    </item>
    <item android:id="@android:id/background"
        android:drawable="@drawable/seekbar_dashes">
    </item>
<!--    <item android:id="@android:id/background"-->
<!--        android:drawable="@drawable/seekbar_dashes2">-->
<!--    </item>-->

    <item android:id="@android:id/progress"
        android:top="30dp"
        android:bottom="30dp">
        <clip>
            <shape android:shape="rectangle">
                <size android:height="16dp"/>
                <solid android:color="@color/design_default_color_secondary"/>
                <corners android:radius="5dp"/>
            </shape>
        </clip>
    </item>

<!--    <item android:id="@android:id/secondaryProgress"-->
<!--        android:drawable="@android:drawable/progress_secondary_holo_dark"/>-->
</layer-list>